{
  "name": "starland-ai",
  "version": "0.1.0",
  "private": true,
  "description": "starland-ai application",
  "author": "starLandAI",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "create-app": "node command/create-app.js",
    "test:unit": "vitest run",
    "test:tag": "node gitJS/tagTest.js",
    "test": "npm run test:unit && npm run test:tag",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:build": "cross-env NODE_ENV=production && npm run build && electron-builder"
  },
  "build": {
    "appId": "com.test.vue",
    "productName": "myTest",
    "directories": {
      "output": "electron-dist"
    },
    "files": [
      "main.js",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "axios": "^1.13.4",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
