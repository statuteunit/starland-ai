{
  "name": "starland-ai",
  "version": "0.1.0",
  "private": true,
  "description": "starland-ai application",
  "author": "starLandAI",
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "create-app": "node command/create-app.js",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:tag": "node gitJS/tagTest.js",
    "test": "yarn run test:unit && yarn run test:tag",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:build": "cross-env NODE_ENV=production && yarn run build && electron-builder"
  },
  "build": {
    "appId": "com.test.vue",
    "productName": "myTest",
    "directories": {
      "output": "electron-dist"
    },
    "files": [
      "main.js",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "axios": "^1.13.4",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.24",
    "babel-plugin-react-compiler": "1.0.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-pxtorem": "^6.1.0",
    "tailwindcss": "3.3.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
